<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Regress"  >

    <test name="ChromeTest">
        <parameter name="browser" value="Chrome" />
        <classes>
            <class name="ProjectFailTest"></class>
            <class name="ProjectSuccessTest"></class>
            <class name="SignUpFailTest"></class>
            <class name="SignUpSuccessTest"></class>
            <class name="TaskSuccessTest"></class>
        </classes>
    </test>

    <test name="EdgeTest">
        <parameter name="browser" value="Edge" />
        <classes>
            <class name="ProjectFailTest"></class>
            <class name="ProjectSuccessTest"></class>
            <class name="SignUpFailTest"></class>
            <class name="SignUpSuccessTest"></class>
            <class name="TaskSuccessTest"></class>
        </classes>
    </test>
</suite>


